{
  "name": "@leomotors/config",
  "description": "My personal config for node projects, include ESLint Prettier and TSConfig",
  "version": "0.2.0-experimental.1",
  "license": "MIT",
  "author": {
    "name": "Nutthapat Pongtanyavichai",
    "url": "https://github.com/Leomotors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Leomotors/music-bot"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint-config",
    "prettier-config",
    "tsconfig"
  ],
  "exports": {
    "./eslint": "./dist/eslint.js",
    "./prettier": "./dist/prettier/index.js",
    "./prettier/importOrders": "./dist/prettier/importOrders.js"
  },
  "typesVersions": {
    "*": {
      "eslint": [
        "./dist/eslint.d.ts"
      ],
      "prettier": [
        "./dist/prettier/index.d.ts"
      ],
      "prettier/importOrders": [
        "./dist/prettier/importOrders.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "prepack": "rm -rf dist && pnpm build"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.15",
    "@types/prettier": "^2.7.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.15",
    "@types/prettier": "^2.7.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "peerDependenciesMeta": {
    "@trivago/prettier-plugin-sort-imports": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    }
  }
}