{
  "name": "@leomotors/config",
  "description": "My personal config for node projects, include ESLint Prettier and TSConfig",
  "version": "0.3.0",
  "license": "MIT",
  "author": {
    "name": "Nutthapat Pongtanyavichai",
    "url": "https://github.com/Leomotors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Leomotors/music-bot"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint-config",
    "prettier-config",
    "tsconfig"
  ],
  "exports": {
    "./eslint": "./dist/eslint.js",
    "./prettier": "./dist/prettier/index.js"
  },
  "typesVersions": {
    "*": {
      "eslint": [
        "./dist/eslint.d.ts"
      ],
      "prettier": [
        "./dist/prettier/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "prepack": "rm -rf dist && pnpm build && pnpm lint",
    "lint-strict": "eslint src --max-warnings=0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.1",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.15",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.1",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.15",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "peerDependenciesMeta": {
    "@ianvs/prettier-plugin-sort-imports": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    }
  },
  "pnpm": {
    "overrides": {
      "@babel/parser": "latest"
    }
  }
}